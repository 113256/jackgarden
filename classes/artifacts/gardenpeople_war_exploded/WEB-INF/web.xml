  <display-name>GardenPeople</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>



  <servlet>
    <servlet-name>gardenpeople.servlet.ProfilePhotosServlet</servlet-name>
    <servlet-class>gardenpeople.servlet.ProfilePhotosServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>gardenpeople.servlet.ProfilePhotosServlet</servlet-name>
    <url-pattern>/photos</url-pattern>
  </servlet-mapping>


</web-app>